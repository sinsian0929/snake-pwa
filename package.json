{
  "name": "snake-pwa",
  "private": true,
  "version": "1.0.0",
  "description": "Premium Snake Game - Cyber Neon Edition",
  "author": "Antigravity",
  "main": "electron-main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "electron .",
    "electron:build": "vite build && electron-builder"
  },
  "devDependencies": {
    "electron": "^33.0.2",
    "electron-builder": "^25.1.8",
    "vite": "^7.3.1"
  },
  "build": {
    "appId": "com.elvis.snakepremium",
    "productName": "Snake Premium",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "electron-main.cjs",
      "package.json"
    ],
    "win": {
      "target": "portable",
      "certificateFile": null
    }
  }
}